{
  "expo": {
    "name": "Ai: Planner",
    "slug": "aitodo",
    "version": "2.0.1",
    "description": "ADHD Todo, AI Task and Planning Assistant",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aiplanner.com",
      "buildNumber": "16",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to scan notes for easy digital storage and management.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to record voice notes and convert speech to text for creating todos and tasks.",
        "NSSpeechRecognitionUsageDescription": "This app needs access to speech recognition to convert your voice to text for creating todos and tasks."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "RECORD_AUDIO",
        "CAMERA",
        "RECORD_AUDIO",
        "CAMERA"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2e591489-5ee9-449b-aec9-44c2ebfc72ca"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/2e591489-5ee9-449b-aec9-44c2ebfc72ca"
    },
    "owner": "harshalone"
  }
}
